{
  "name": "react-node-graphql",
  "version": "1.0.0",
  "description": "React Node GraphQL",
  "scripts": {
    "clean": "rimraf build",
    "build:client": "cross-env NODE_ENV=production webpack --mode production --config webpack/webpack.prod.js",
    "build:server": "tsc -p ./server/tsconfig.json --outDir build/server && tsc-alias -p ./server/tsconfig.json",
    "build": "npm run clean && yarn build:client && yarn build:server && yarn copy",
    "copy": "cp server/graphql/schemas/*.graphql build/server/graphql/schemas",
    "dev:client": "cross-env NODE_ENV=development webpack-dev-server --config webpack/webpack.dev.js",
    "dev:server": "nodemon",
    "dev": "npm-run-all --parallel dev:*",
    "start": "cross-env NODE_ENV=production node build/server/index.js",
    "postinstall": "yarn gql-codegen",
    "gql-codegen": "graphql-codegen --config codegen.yml"
  },
  "keywords": [
    "react",
    "node",
    "typescript",
    "graphql",
    "@graphql-codegen",
    "@graphql-tools",
    "apollographql"
  ],
  "author": "Mohamed Samara (https://github.com/mohamedsamara)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mohamedsamara/react-node-graphql/issues"
  },
  "homepage": "https://github.com/mohamedsamara/react-node-graphql#readme",
  "dependencies": {
    "apollo-server-express": "^2.22.2",
    "bootstrap": "^5.1.3",
    "dotenv": "^16.0.0",
    "express": "^4.17.1",
    "graphql": "^15.5.0",
    "pg": "^8.7.3",
    "pg-hstore": "^2.3.4",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^6.3.0",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^6.21.0",
    "sequelize-typescript": "^2.1.3",
    "simple-line-icons": "^2.5.5",
    "workbox-core": "^6.5.3",
    "workbox-precaching": "^6.5.3",
    "workbox-routing": "^6.5.3"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^1.21.4",
    "@graphql-codegen/introspection": "^1.18.2",
    "@graphql-codegen/typescript": "^1.22.0",
    "@graphql-codegen/typescript-operations": "^1.17.16",
    "@graphql-codegen/typescript-react-apollo": "^2.2.4",
    "@graphql-codegen/typescript-resolvers": "^1.19.1",
    "@graphql-tools/load-files": "^6.5.4",
    "@graphql-tools/merge": "^8.2.14",
    "@types/express": "^4.17.11",
    "@types/node": "^14.14.35",
    "@types/pg": "^8.6.5",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.3",
    "@types/react-router-dom": "^5.3.3",
    "@typescript-eslint/eslint-plugin": "^5.18.0",
    "@typescript-eslint/parser": "^5.18.0",
    "autoprefixer": "^10.4.4",
    "copy-webpack-plugin": "^10.2.4",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.1",
    "eslint": "^8.13.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.4.0",
    "eslint-webpack-plugin": "^3.1.1",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.0",
    "mini-css-extract-plugin": "^2.6.0",
    "nodemon": "^2.0.15",
    "npm-run-all": "^4.1.5",
    "postcss-loader": "^6.2.1",
    "postcss-normalize": "^10.0.1",
    "sass": "^1.50.0",
    "sass-loader": "^12.6.0",
    "style-loader": "^3.3.1",
    "terser-webpack-plugin": "^5.3.1",
    "ts-loader": "^8.0.18",
    "ts-node": "^10.7.0",
    "tsc-alias": "^1.6.11",
    "tsconfig-paths": "^4.0.0",
    "typescript": "^4.2.3",
    "webpack": "^5.26.3",
    "webpack-cli": "^4.5.0",
    "webpack-dev-server": "^4.8.1",
    "webpack-merge": "^5.8.0",
    "webpack-pwa-manifest": "^4.3.0",
    "workbox-webpack-plugin": "^6.5.3"
  }
}
